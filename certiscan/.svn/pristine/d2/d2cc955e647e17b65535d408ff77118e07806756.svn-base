package com.certicom.certiscan.domain;

public class CategoriaIndicadoresProducto {
	
	private Integer id_cat_producto;
	private String descripcion;
	private Boolean estado;
	
	public Integer getId_cat_producto() {
		return id_cat_producto;
	}
	public void setId_cat_producto(Integer id_cat_producto) {
		this.id_cat_producto = id_cat_producto;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public Boolean getEstado() {
		return estado;
	}
	public void setEstado(Boolean estado) {
		this.estado = estado;
	}
	
	

}
