							<p:commandButton id="cerrarPlanilla" value="Cerrar Planilla" update="@form" disabled="#{facturacionComisionesMB.btnCerrarPlanilla}" actionListener="#{facturacionComisionesMB.cerrarPlanilla()}" icon="icon-add"
