package com.certicom.certiscan.domain;

import java.util.List;

import net.sf.jasperreports.engine.data.JRBeanCollectionDataSource;

public class CategoriaIndicadoresIndice {
	
	private Integer id_categoria_call;
	private String descripcion;
	private String orden;
	private boolean estado;
	private List<Indice> listIndicadorIndice;
	
	public CategoriaIndicadoresIndice(){
	}
	
	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public boolean isEstado() {
		return estado;
	}

	public void setEstado(boolean estado) {
		this.estado = estado;
	}

	public Integer getId_categoria_call() {
		return id_categoria_call;
	}

	public void setId_categoria_call(Integer id_categoria_call) {
		this.id_categoria_call = id_categoria_call;
	}


	public List<Indice> getListIndicadorIndice() {
		return listIndicadorIndice;
	}

	public void setListIndicadorIndice(List<Indice> listIndicadorIndice) {
		this.listIndicadorIndice = listIndicadorIndice;
	}

	public String getOrden() {
		return orden;
	}

	public void setOrden(String orden) {
		this.orden = orden;
	}


}
