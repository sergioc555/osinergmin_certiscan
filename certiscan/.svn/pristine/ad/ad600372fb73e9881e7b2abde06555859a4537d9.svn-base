package com.certicom.certiscan.domain;

import java.util.List;

public class Estados {

	private Integer id_estado;
	private Integer id_estado_padre;
	private Integer id_categoria_estado;
	private String descripcion;
	private String flag_banco;
	private String descripcion_banco;

	private boolean estado;
	private String des_categoria_estados;
	private Integer idProducto;

	private String des_padre;

	private String codigo_categoria;
	private String codigo_estado;
	private String codigo_estado_padre;

	private List<Estados> subfamilias;

	public Estados() {

	}

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public boolean isEstado() {
		return estado;
	}

	public void setEstado(boolean estado) {
		this.estado = estado;
	}

	public Integer getId_estado() {
		return id_estado;
	}

	public void setId_estado(Integer id_estado) {
		this.id_estado = id_estado;
	}

	public Integer getId_categoria_estado() {
		return id_categoria_estado;
	}

	public void setId_categoria_estado(Integer id_categoria_estado) {
		this.id_categoria_estado = id_categoria_estado;
	}

	public String getDes_categoria_estados() {
		return des_categoria_estados;
	}

	public void setDes_categoria_estados(String des_categoria_estados) {
		this.des_categoria_estados = des_categoria_estados;
	}

	public Integer getIdProducto() {
		return idProducto;
	}

	public void setIdProducto(Integer idProducto) {
		this.idProducto = idProducto;
	}

	public String getFlag_banco() {
		return flag_banco;
	}

	public void setFlag_banco(String flag_banco) {
		this.flag_banco = flag_banco;
	}

	public String getDescripcion_banco() {
		return descripcion_banco;
	}

	public void setDescripcion_banco(String descripcion_banco) {
		this.descripcion_banco = descripcion_banco;
	}

	public Integer getId_estado_padre() {
		return id_estado_padre;
	}

	public void setId_estado_padre(Integer id_estado_padre) {
		this.id_estado_padre = id_estado_padre;
	}

	public String getDes_padre() {
		return des_padre;
	}

	public void setDes_padre(String des_padre) {
		this.des_padre = des_padre;
	}

	public String getCodigo_categoria() {
		return codigo_categoria;
	}

	public void setCodigo_categoria(String codigo_categoria) {
		this.codigo_categoria = codigo_categoria;
	}

	public String getCodigo_estado() {
		return codigo_estado;
	}

	public void setCodigo_estado(String codigo_estado) {
		this.codigo_estado = codigo_estado;
	}

	public String getCodigo_estado_padre() {
		return codigo_estado_padre;
	}

	public void setCodigo_estado_padre(String codigo_estado_padre) {
		this.codigo_estado_padre = codigo_estado_padre;
	}

	public List<Estados> getSubfamilias() {
		return subfamilias;
	}

	public void setSubfamilias(List<Estados> subfamilias) {
		this.subfamilias = subfamilias;
	}

}
