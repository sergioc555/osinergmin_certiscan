
/* hace bloque a un objeto */
.block{
	display: block !important;
}

/* elimina los bordes de un objeto */
.noBorder, .ui-panelgrid.noBorder,.ui-panelgrid.noBorder>table,.ui-panelgrid.noBorder>tbody, .ui-panelgrid.noBorder>th, .ui-panelgrid.noBorder>tbody>tr,  .ui-panelgrid.noBorder>tbody>tr.ui-widget-content, .ui-panelgrid.noBorder>th>td, , .ui-panelgrid.noBorder>tbody>tr>td{
background: none !important;
border: none !important;	
}

/* pone anchos a objetos */
